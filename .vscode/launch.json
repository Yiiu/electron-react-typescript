{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Electron Main",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
      "program": "${workspaceRoot}/src/main/main.ts",
      "outFiles": [
          "${workspaceRoot}/dist/*.js"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    },
  ]
}
